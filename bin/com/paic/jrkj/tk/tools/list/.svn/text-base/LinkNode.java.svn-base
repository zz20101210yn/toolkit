final class LinkNode<E> {

    private volatile LinkNode<E> prev;
    private volatile LinkNode<E> next;
    private final E value;

    public LinkNode(E value) {
        this.value = value;
    }

    public LinkNode<E> getPrev() {
        return prev;
    }

    public void setPrev(LinkNode<E> prev) {
        this.prev = prev;
    }

    public LinkNode<E> getNext() {
        return next;
    }

    public void setNext(LinkNode<E> next) {
        this.next = next;
    }

    public E getValue() {
        return value;
    }
}
